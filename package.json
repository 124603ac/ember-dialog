{
  "name": "ember-dialog",
  "version": "3.0.0-rc.8",
  "description": "Ember Dialog is a simplest way to create dialogs",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:testall",
    "deploy": "./publish.sh",
    "generate-coverage": "COVERAGE=true ember test",
    "coverage": "npm run generate-coverage && cat ./coverage/lcov.info | CODACY_PROJECT_TOKEN=ca5159a3391d43db90a053c83cb34eea ./node_modules/.bin/codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wheely/ember-dialog.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": {
    "name": "Vladimir Milkov",
    "email": "v.v.milkov@gmail.com",
    "url": "https://github.com/ajile/"
  },
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "codacy-coverage": "^1.1.3",
    "ember-cli": "2.5.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-code-coverage": "0.2.2",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-doc-server": "1.1.0",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-jsdoc": "1.4.0",
    "ember-cli-qunit": "^1.0.4",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^1.2.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "1.13.15",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-i18n": "4.1.3",
    "ember-try": "~0.0.8",
    "frontend-basis": "wheely/frontend-basis#v3.0.0",
    "ink-docstrap": "^1.2.1",
    "loader.js": "^4.0.11"
  },
  "keywords": [
    "ember-addon",
    "dialog",
    "modal",
    "window",
    "popup"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-sass": "^4.2.0"
  },
  "ember-addon": {
    "demoURL": "http://wheely.github.io/ember-dialog/",
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">1.10.0"
    }
  }
}
